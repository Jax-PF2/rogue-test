ug":null,"schema":{"version":0.815,"lastMigration":null},"traits":{"value":["rogue"],"rarity":"common","custom":""},"actionType":{"value":"action"},"category":null,"actions":{"value":2},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"actionCategory":{"value":""}},"img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/actions/TwoActions.webp","effects":[],"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Item.Dg2l33jCvURsrjOW"}},"_stats":{"systemId":"pf2e","systemVersion":"4.6.8","coreVersion":"10.291","createdTime":1676139567063,"modifiedTime":1676151773767,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"_id":"PLy5IRPTfZyA2Ldc"},{"name":"Poison Weapon","type":"action","system":{"description":{"gm":"","value":"<p><strong>Requirement: </strong>The rogue is wielding a piercing or slashing weapon.</p>\n<p><strong>Effect: </strong>The rogue applies a poison to the required weapon; if they have a free hand, they can Interact to draw a poison as part of this action. If their next attack with that weapon before the end of their next turn hits and deals damage, it applies the effects of the poison, provided that poison can be delivered by contact or injury. If they critically fail the attack roll, the poison is wasted as normal.</p>\n<p> </p>\n<p>Ability level: 1-4</p>"},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.815,"lastMigration":null},"traits":{"value":["rogue"],"rarity":"common","custom":""},"actionType":{"value":"action"},"category":null,"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"actionCategory":{"value":""}},"img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/actions/OneAction.webp","effects":[],"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Item.k01pPV1gF1WVsnHf"}},"_stats":{"systemId":"pf2e","systemVersion":"4.6.8","coreVersion":"10.291","createdTime":1675778132687,"modifiedT